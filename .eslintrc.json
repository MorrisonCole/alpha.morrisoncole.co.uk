{
  "extends": "next/core-web-vitals",
  "plugins": [
    "formatjs"
  ],
  "rules": {
    "sort-imports": "error",
    "formatjs/enforce-default-message": [
      "error",
      "literal"
    ],
    "formatjs/enforce-id": [
      "error",
      {
        "idInterpolationPattern": "[sha512:contenthash:base64:6]"
      }
    ]
  }
}
