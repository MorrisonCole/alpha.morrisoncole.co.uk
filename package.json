    "gray-matter": "4.0.3",
    "next": "13.2.4",
    "next-mdx-remote": "4.4.1",
    "prettier": "2.8.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.3.2",
    "react-is": "18.2.0",
