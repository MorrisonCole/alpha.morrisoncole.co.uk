  "devDependencies": {
    "@formatjs/cli": "6.0.4",
    "@formatjs/swc-plugin": "1.4.20",
    "@lhci/cli": "0.11.0",
    "@types/mdx": "2.0.3",
    "@types/node": "18.15.10",
    "@types/react": "18.0.29",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "eslint-plugin-formatjs": "4.9.0",
    "glyphhanger": "5.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "typescript": "5.0.2"
  },
  "resolutions": {
    "@types/react": "18.0.28"
  },
  "scripts": {
    "dev": "run i18n:compile && next dev",
    "build": "run i18n:compile && next build",
    "start": "next start",
    "lint": "next lint",
    "test:lighthouse": "lhci autorun",
    "fonts": "glyphhanger http://localhost:3000/ --spider --family='Noto Sans' --subset='*.ttf' --formats=woff2 --css",
    "get-fonts": "ts-node -T scripts/get-fonts.ts",
    "upgrade": "yarn set version latest",
    "i18n:extract": "formatjs extract 'pages/**/*.ts*' 'components/**/*.ts*' --out-file lang/en.json",
    "i18n:compile": "formatjs compile-folder lang compiled-lang",
    "postinstall": "husky install"
  },
  "packageManager": "yarn@3.5.0"
}
