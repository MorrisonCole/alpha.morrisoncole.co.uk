    "@mdx-js/react": "2.3.0",
    "@next/mdx": "13.4.6",
    "@types/css": "0.0.33",
    "@types/eslint": "8.37.0",
    "@types/node": "20.3.1",
    "@types/prettier": "2.7.3",
    "@typescript-eslint/eslint-plugin": "5.59.9",
    "@typescript-eslint/parser": "5.59.2",
    "axios": "1.4.0",
    "css": "3.0.0",
    "date-fns": "2.30.0",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-react": "7.32.2",
    "next": "13.4.1",
    "prettier": "2.8.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.4.2",
    "react-is": "18.2.0",
    "styled-components": "5.3.11",
    "ts-node": "10.9.1",
    "typescript": "5.1.3",
    "vercel": "30.2.2"
  },
  "devDependencies": {
    "@formatjs/cli": "6.1.3",
    "@formatjs/swc-plugin": "1.5.3",
    "@lhci/cli": "0.12.0",
    "@types/mdx": "2.0.5",
    "@types/node": "20.3.1",
    "@types/react-dom": "18.2.3",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.59.9",
    "@typescript-eslint/parser": "5.59.2",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.5",
    "eslint-plugin-formatjs": "4.10.2",
    "glyphhanger": "5.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "typescript": "5.1.3",
    "webpack": "5.87.0"
  },
  "scripts": {
    "dev": "run i18n:compile && next dev",
    "build": "run i18n:compile && next build",
    "start": "next start",
    "lint": "next lint",
    "test:lighthouse": "lhci autorun",
    "fonts": "glyphhanger http://localhost:3000/ --spider --family='Noto Sans' --subset='*.ttf' --formats=woff2 --css",
    "get-fonts": "ts-node -T scripts/get-fonts.ts",
    "upgrade": "yarn set version latest",
    "i18n:extract": "formatjs extract 'pages/**/*.ts*' 'components/**/*.ts*' --out-file lang/en.json",
    "i18n:compile": "formatjs compile-folder lang compiled-lang",
    "postinstall": "husky install"
  },
  "packageManager": "yarn@3.6.0"
}
