    "axios": "1.5.0",
    "css": "3.0.0",
    "date-fns": "2.30.0",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-react": "7.33.2",
    "next": "13.4.19",
    "prettier": "3.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.4.5",
    "react-is": "18.2.0",
    "styled-components": "6.0.7",
    "ts-node": "10.9.1",
