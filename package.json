  "devDependencies": {
    "@formatjs/cli": "6.1.4",
    "@formatjs/swc-plugin": "1.5.4",
    "@lhci/cli": "0.12.0",
    "@types/mdx": "2.0.7",
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "@typescript-eslint/parser": "6.6.0",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "eslint-plugin-formatjs": "4.10.4",
    "glyphhanger": "5.0.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "typescript": "5.2.2"
  },
  "resolutions": {
    "@types/react": "18.2.21"
  },
  "scripts": {
    "dev": "run i18n:compile && next dev",
    "build": "run i18n:compile && next build",
    "start": "next start",
    "lint": "next lint",
    "test:lighthouse": "lhci autorun",
    "fonts": "glyphhanger http://localhost:3000/ --spider --family='Noto Sans' --subset='*.ttf' --formats=woff2 --css",
    "get-fonts": "ts-node -T scripts/get-fonts.ts",
    "upgrade": "yarn set version latest",
    "i18n:extract": "formatjs extract 'pages/**/*.ts*' 'components/**/*.ts*' --out-file lang/en.json",
    "i18n:compile": "formatjs compile-folder lang compiled-lang",
    "postinstall": "husky install"
  },
  "packageManager": "yarn@3.6.3"
}
